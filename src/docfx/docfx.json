{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Tmds.Ssh/Tmds.Ssh.csproj"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "*.md"
        ],
        "exclude": [
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "src": "../icon",
        "files": [
          "icon.svg",
          "icon_128.png"
        ]
      }
    ],
    "output": "../../_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appTitle": "Tmds.Ssh API Documentation",
      "_appName": "Tmds.Ssh",
      "_appLogoPath": "icon.svg",
      "_appFaviconPath": "icon.svg",
      "_enableSearch": true
    }
  }
}
